{
    "name": "sven-reichert/laravel-visitor-tracker",
    "description": "Visitor tracker and statistics for Laravel 5/6",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Zavyalov",
            "email": "alex@voerro.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.* | 5.6.* | 5.7.* | 5.8.* | 6.0.* | 6.1.* | 6.2.* | 6.3.* | 6.4.* | 6.5.* | 6.6.* | 6.7.* | 7.5*",
        "illuminate/http": "5.5.* | 5.6.* | 5.7.* | 5.8.* | 6.0.* | 6.1.* | 6.2.* | 6.3.* | 6.4.* | 6.5.* | 6.6.* | 6.7.* | 7.5*",
        "illuminate/database": "5.5.* | 5.6.* | 5.7.* | 5.8.* | 6.0.* | 6.1.* | 6.2.* | 6.3.* | 6.4.* | 6.5.* | 6.6.* | 6.7.* | 7.5*",
        "illuminate/bus": "5.5.* | 5.6.* | 5.7.* | 5.8.* | 6.0.* | 6.1.* | 6.2.* | 6.3.* | 6.4.* | 6.5.* | 6.6.* | 6.7.* | 7.5*",
        "illuminate/queue": "5.5.* | 5.6.* | 5.7.* | 5.8.* | 6.0.* | 6.1.* | 6.2.* | 6.3.* | 6.4.* | 6.5.* | 6.6.* | 6.7.* | 7.5*",
        "illuminate/contracts": "5.5.* | 5.6.* | 5.7.* | 5.8.* | 6.0.* | 6.1.* | 6.2.* | 6.3.* | 6.4.* | 6.5.* | 6.6.* | 6.7.* | 7.5*",
        "piwik/device-detector": "^3.12",
        "guzzlehttp/guzzle": "~6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 | ^7.0 | ^8.0",
        "orchestra/testbench": "^3.5 | ^3.6 | ^3.7 | ^3.8 | ^4.0 | ^5.1 "
    },
    "autoload": {
        "psr-4": {
            "Voerro\\Laravel\\VisitorTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Voerro\\Laravel\\VisitorTracker\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voerro\\Laravel\\VisitorTracker\\VisitorTrackerServiceProvider"
            ],
            "aliases": {
                "VisitStats": "Voerro\\Laravel\\VisitorTracker\\Facades\\VisitStats"
            }
        }
    }
}
